{
  "version": 2,
  "builds": [
    {
      "src": "./api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/order", "dest": "/api/index.js" },
    { "src": "/api/order/validate", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
